modelo Bmi:
    contrutor(altura, peso):
        Bmi.altura = altura
        Bmi.peso = peso

    funcao calcularBMI():
        total = peso/(altura*altura)
        imprime(total)

    
enquanto valido == false:
    entrada("Insira o seu peso em KG", peso)
    entrada("Insira a sua altura em M", altura)
    se 0 > peso E 0 > altura > 3 E altura NAO FLOAT:
        imprime("Dados invalidos. Por favor tente novamente.")
        continua()
    bmi = Bmi(peso, altura)
    imprime(bmi.calcularBMI())