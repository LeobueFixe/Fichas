entrada("Insira um número positivo:", numero)

se numero NÃO INT OU numero < 0:
    imprime("Tente novamente:")
senao:

    contador = 1
    total = 0

    enquanto total < numero:
        total = 0
        total = contador ^ contador
        contador += 0.1
    
    se total == numero:
        imprime("A raiz quadrada do número é {contador}.")
    senao total > numero:
        imprime("O numero ")
