lista = []
total = 0

enquanto tamanho(lista) < 3:
    entrada("Insira uma nota", nota)
    se nota NAO INT OU nota NAO FLOAT:
        entrada("Nota invalida, por favor tente novamente:", nota)
        continua()
    lista.adicionar(nota)
    total += nota

media = total /  tamanho(lista)

se media => 7:
    imprime("O aluno foi aprovado")
senao:
    imprime("O aluno reprovou")
